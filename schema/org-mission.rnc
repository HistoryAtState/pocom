element org-mission {
    element id { xsd:NCName },
    element category { xsd:NCName },
    element names {
        element singular { text },
        element plural { text }
    },
    element description {
        element div {
            element p { text }
        }
    },
    element chiefs {
        (element chief {
             element id { xsd:NCName },
             element person-id { xsd:NCName },
             element role-title-id { xsd:NCName },
             element appointed {
                 element date {
                     attribute certainty { "high" }?,
                     text
                 },
                 element note { text }
             },
             element arrived {
                 element date {
                     attribute certainty { "high" }?,
                     text
                 },
                 element note { text }
             },
             element started {
                 element date {
                     attribute certainty { "high" }?,
                     text
                 },
                 element note { text }
             },
             element ended {
                 element date {
                     attribute certainty { "high" }?,
                     text
                 },
                 element note { text }
             },
             element note { text }*,
             element created-by { xsd:NCName },
             element created-date { xsd:date },
             element last-modified-by { xsd:NCName },
             element last-modified-date { xsd:date }
         }
         | element mission-note {
               element text { text },
               element created-by { xsd:NCName },
               element created-date { xsd:date },
               element last-modified-by { xsd:NCName },
               element last-modified-date { xsd:date }
           })+
    },
    element other-appointees {
        element chief {
            element id { xsd:NCName },
            element person-id { xsd:NCName },
            element role-title-id { xsd:NCName },
            element appointed {
                element date {
                    attribute certainty { "high" }?,
                    text
                },
                element note { text }
            },
            element arrived {
                 element date {
                     attribute certainty { "high" }?,
                     text
                 },
                 element note { text }
             },
            element started {
                element date {
                    attribute certainty { "high" }?,
                    text
                },
                element note { text }
            },
            element ended {
                element date {
                    attribute certainty { "high" }?,
                    text
                },
                element note { text }
            },
            element note { text }*,
            element created-by { xsd:NCName },
            element created-date { xsd:date },
            element last-modified-by { xsd:NCName },
            element last-modified-date { xsd:date }
        }*
    }
}
